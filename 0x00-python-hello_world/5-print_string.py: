#!/usr/bin/python3
secreteWord = input("Enter your secrete word:")
if isinstance(secreteWord , str):
    print("Input is a string.")
    print(secreteWord * 3)
    if len(secreteWord) >= 9:
            print(secreteWord[:9])
    else:
            print("Input string does not have more than 9 characters.")
else:
    print("Input is not a string.")

